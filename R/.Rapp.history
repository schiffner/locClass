all classes, correct order#
	data(iris)#
	iris[,1:4] <- scale(iris[,1:4])#
	k <- 100#
	n <- 90#
	x <- as.matrix(iris[,-5])#
	dist <- sqrt(colSums((t(x) - x[n,])^2))#
	w <- rectangular(k = k)(dist)#
