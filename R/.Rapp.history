setMethod("determinePrior", signature(concomitant = "FLXPmultinom"), function (prior, concomitant, group) {#
		x <- concomitant@x %*% concomitant@coef#
		m <- apply(x, 1, max)#
		exps <- exp(x - m)#
		exps/rowSums(exps)#
})
